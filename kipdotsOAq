[32m+[m[32m  printf %s "(cached) " >&6[m
[32m+[m[32melse case e in #([m
[32m+[m[32m  e)     # Double quotes because $CC needs to be expanded[m
[32m+[m[32m    for CPP in "$CC -E" "$CC -E -traditional-cpp" cpp /lib/cpp[m
[32m+[m[32m    do[m
[32m+[m[32m      ac_preproc_ok=false[m
[32m+[m[32mfor ac_c_preproc_warn_flag in '' yes[m
[32m+[m[32mdo[m
[32m+[m[32m  # Use a header file that comes with gcc, so configuring glibc[m
[32m+[m[32m  # with a fresh cross-compiler works.[m
[32m+[m[32m  # On the NeXT, cc -E runs the code through the compiler's parser,[m
[32m+[m[32m  # not just through cpp. "Syntax error" is here to catch this case.[m
[32m+[m[32m  cat confdefs.h - <<_ACEOF >conftest.$ac_ext[m
[32m+[m[32m/* end confdefs.h.  */[m
[32m+[m[32m#include <limits.h>[m
[32m+[m		[32m     Syntax error[m
[32m+[m[32m_ACEOF[m
[32m+[m[32mif ac_fn_c_try_cpp "$LINENO"[m
[32m+[m[32mthen :[m
[32m+[m
[32m+[m[32melse case e in #([m
[32m+[m[32m  e) # Broken: fails on valid input.[m
[32m+[m[32mcontinue ;;[m
[32m+[m[32mesac[m
[32m+[m[32mfi[m
[32m+[m[32mrm -f conftest.err conftest.i conftest.$ac_ext[m
[32m+[m
[32m+[m[32m  # OK, works on sane cases.  Now check whether nonexistent headers[m
[32m+[m[32m  # can be detected and how.[m
[32m+[m[32m  cat confdefs.h - <<_ACEOF >conftest.$ac_ext[m
[32m+[m[32m/* end confdefs.h.  */[m
[32m+[m[32m#include <ac_nonexistent.h>[m
[32m+[m[32m_ACEOF[m
[32m+[m[32mif ac_fn_c_try_cpp "$LINENO"[m
[32m+[m[32mthen :[m
[32m+[m[32m  # Broken: success on invalid input.[m
[32m+[m[32mcontinue[m
[32m+[m[32melse case e in #([m
[32m+[m[32m  e) # Passes both tests.[m
[32m+[m[32mac_preproc_ok=:[m
[32m+[m[32mbreak ;;[m
[32m+[m[32mesac[m
[32m+[m[32mfi[m
[32m+[m[32mrm -f conftest.err conftest.i conftest.$ac_ext[m
[32m+[m
[32m+[m[32mdone[m
[32m+[m[32m# Because of 'break', _AC_PREPROC_IFELSE's cleaning code was skipped.[m
[32m+[m[32mrm -f conftest.i conftest.err conftest.$ac_ext[m
[32m+[m[32mif $ac_preproc_ok[m
[32m+[m[32mthen :[m
[32m+[m[32m  break[m
[32m+[m[32mfi[m
[32m+[m
[32m+[m[32m    done[m
[32m+[m[32m    ac_cv_prog_CPP=$CPP[m
[32m+[m[32m   ;;[m
[32m+[m[32mesac[m
[32m+[m[32mfi[m
[32m+[m[32m  CPP=$ac_cv_prog_CPP[m
[32m+[m[32melse[m
[32m+[m[32m  ac_cv_prog_CPP=$CPP[m
[32m+[m[32mfi[m
[32m+[m[32m{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $CPP" >&5[m
[32m+[m[32mprintf "%s\n" "$CPP" >&6; }[m
[32m+[m[32mac_preproc_ok=false[m
[32m+[m[32mfor ac_c_preproc_warn_flag in '' yes[m
[32m+[m[32mdo[m
[32m+[m[32m  # Use a header file that comes with gcc, so configuring glibc[m
[32m+[m[32m  # with a fresh cross-compiler works.[m
[32m+[m[32m  # On the NeXT, cc -E runs the code through the compiler's parser,[m
[32m+[m[32m  # not just through cpp. "Syntax error" is here to catch this case.[m
[32m+[m[32m  cat confdefs.h - <<_ACEOF >conftest.$ac_ext[m
[32m+[m[32m/* end confdefs.h.  */[m
[32m+[m[32m#include <limits.h>[m
[32m+[m		[32m     Syntax error[m
[32m+[m[32m_ACEOF[m
[32m+[m[32mif ac_fn_c_try_cpp "$LINENO"[m
[32m+[m[32mthen :[m
[32m+[m
[32m+[m[32melse case e in #([m
[32m+[m[32m  e) # Broken: fails on valid input.[m
[32m+[m[32mcontinue ;;[m
[32m+[m[32mesac[m
[32m+[m[32mfi[m
[32m+[m[32mrm -f conftest.err conftest.i conftest.$ac_ext[m
[32m+[m
[32m+[m[32m  # OK, works on sane cases.  Now check whether nonexistent headers[m
[32m+[m[32m  # can be detected and how.[m
[32m+[m[32m  cat confdefs.h - <<_ACEOF >conftest.$ac_ext[m
[32m+[m[32m/* end confdefs.h.  */[m
[32m+[m[32m#include <ac_nonexistent.h>[m
[32m+[m[32m_ACEOF[m
[32m+[m[32mif ac_fn_c_try_cpp "$LINENO"[m
